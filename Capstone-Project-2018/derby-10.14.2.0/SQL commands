CREATE TABLE Users (
UserID int NOT NULL,
LastName varchar(255) NOT NULL,
FirstName varchar(255) NOT NULL,
Email varchar (255) NOT NULL,
Username varchar (25) NOT NULL,
Password varchar (255) NOT NULL,
PRIMARY KEY (Email)
);

INSERT INTO Users
(UserID, LastName, FirstName, Email, username, password) 
